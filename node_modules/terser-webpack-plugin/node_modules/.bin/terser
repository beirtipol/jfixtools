#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules/terser/bin/terser/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules/terser/bin/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules/terser/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/terser/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../terser/4.8.0/node_modules/terser/bin/terser" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules/terser/bin/terser/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules/terser/bin/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules/terser/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/terser/4.8.0/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/terser/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../terser/4.8.0/node_modules/terser/bin/terser" "$@"
  ret=$?
fi
exit $ret
