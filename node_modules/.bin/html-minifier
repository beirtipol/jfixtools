#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules/html-minifier/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules/html-minifier/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/html-minifier/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules:/mnt/c/dev/source/jfixtools/node_modules/html-minifier/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/html-minifier/node_modules" "$basedir/node"  "$basedir/../html-minifier/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules/html-minifier/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules/html-minifier/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/html-minifier/3.5.21/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/html-minifier/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules:/mnt/c/dev/source/jfixtools/node_modules/html-minifier/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/html-minifier/node_modules" node  "$basedir/../html-minifier/cli.js" "$@"
  ret=$?
fi
exit $ret
