#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/rimraf/3.0.2/node_modules/rimraf/bin.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/rimraf/3.0.2/node_modules/rimraf/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/rimraf/3.0.2/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/rimraf/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules:/mnt/c/dev/source/jfixtools/node_modules/rimraf/bin.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/rimraf/node_modules" "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/rimraf/3.0.2/node_modules/rimraf/bin.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/rimraf/3.0.2/node_modules/rimraf/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/rimraf/3.0.2/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/rimraf/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules:/mnt/c/dev/source/jfixtools/node_modules/rimraf/bin.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/rimraf/node_modules" node  "$basedir/../rimraf/bin.js" "$@"
  ret=$?
fi
exit $ret
