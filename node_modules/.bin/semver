#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/semver/6.3.0/node_modules/semver/bin/semver.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/semver/6.3.0/node_modules/semver/bin/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/semver/6.3.0/node_modules/semver/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/semver/6.3.0/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/semver/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules:/mnt/c/dev/source/jfixtools/node_modules/semver/bin/semver.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/semver/bin/node_modules:/mnt/c/dev/source/jfixtools/node_modules/semver/node_modules" "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/semver/6.3.0/node_modules/semver/bin/semver.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/semver/6.3.0/node_modules/semver/bin/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/semver/6.3.0/node_modules/semver/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/semver/6.3.0/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/semver/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules:/mnt/c/dev/source/jfixtools/node_modules/semver/bin/semver.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/semver/bin/node_modules:/mnt/c/dev/source/jfixtools/node_modules/semver/node_modules" node  "$basedir/../semver/bin/semver.js" "$@"
  ret=$?
fi
exit $ret
