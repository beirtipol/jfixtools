#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/webpack-cli/3.3.11_webpack@4.42.0/node_modules/webpack-cli/bin/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/webpack-cli/3.3.11_webpack@4.42.0/node_modules/webpack-cli/bin/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/webpack-cli/3.3.11_webpack@4.42.0/node_modules/webpack-cli/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/webpack-cli/3.3.11_webpack@4.42.0/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/webpack-cli/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules:/mnt/c/dev/source/jfixtools/node_modules/webpack-cli/bin/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/webpack-cli/bin/node_modules:/mnt/c/dev/source/jfixtools/node_modules/webpack-cli/node_modules" "$basedir/node"  "$basedir/../webpack-cli/bin/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/webpack-cli/3.3.11_webpack@4.42.0/node_modules/webpack-cli/bin/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/webpack-cli/3.3.11_webpack@4.42.0/node_modules/webpack-cli/bin/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/webpack-cli/3.3.11_webpack@4.42.0/node_modules/webpack-cli/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/webpack-cli/3.3.11_webpack@4.42.0/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/webpack-cli/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules:/mnt/c/dev/source/jfixtools/node_modules/webpack-cli/bin/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/webpack-cli/bin/node_modules:/mnt/c/dev/source/jfixtools/node_modules/webpack-cli/node_modules" node  "$basedir/../webpack-cli/bin/cli.js" "$@"
  ret=$?
fi
exit $ret
