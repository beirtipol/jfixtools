#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules/mime/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules/mime/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/mime/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules:/mnt/c/dev/source/jfixtools/node_modules/mime/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/mime/node_modules" "$basedir/node"  "$basedir/../mime/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules/mime/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules/mime/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/mime/2.4.6/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/mime/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/dev/source/jfixtools/node_modules/.pnpm/node_modules:/mnt/c/dev/source/jfixtools/node_modules:/mnt/c/dev/source/node_modules:/mnt/c/dev/node_modules:/mnt/c/node_modules:/mnt/c/dev/source/jfixtools/node_modules/mime/cli.js/node_modules:/mnt/c/dev/source/jfixtools/node_modules/mime/node_modules" node  "$basedir/../mime/cli.js" "$@"
  ret=$?
fi
exit $ret
